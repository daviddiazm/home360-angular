    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.16",
    "@angular/cli": "^16.2.16",
    "@angular/compiler-cli": "^16.2.0",
    "@types/jest": "^29.5.14",
    "esbuild": "^0.25.3",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.5.5",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
